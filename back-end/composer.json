{
    "name": "felipe/market",
    "require": {
        "php": "^8.3.9",
        "ext-pdo": "*",
        "nesbot/carbon": "^3.6"
    },
    "autoload": {
        "classmap": [
            "app/"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "pestphp/pest": "^2.34",
        "mockery/mockery": "^1.6"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
